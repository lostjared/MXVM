program CountTo10 {
    section data {
        string fmt = "%d\n"
        int v1 = 0
    }
    section code {
        start:
            mov v1, 1
            call count
            done
        function count:
            cmp v1, 11
            je end_count
            print fmt, v1
            add v1, 1
            call count
        end_count:
            ret
    }
}
