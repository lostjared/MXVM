program File {
    section module {
        io
    }
    section data {
        ptr handle = null
        string filename = "test.txt"
        string mode = "w"
        string text = "Hello, World\n"

    }
    section code {
    start:
        invoke fopen, filename, mode
        return handle
        invoke fprintf, handle, text
        invoke fclose, handle
        invoke fprintf, stderr, text
        done
    }
}