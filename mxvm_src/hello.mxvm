program Hello {
	
    section data {
		ptr pointer = null
		float x = 10
		float y = 0
		int amount = 1
		string format = "value is: %f\n"
		string subr = "hey\n"
	}
    
	section code {
	start:
		alloc pointer, 8, amount
		store x, pointer, 0
		load y, pointer, 0
		print format, y
		call hello
	stop:
		free pointer
		#done

	function hello:
		print subr
		ret
	}
}
