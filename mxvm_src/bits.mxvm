program Bits {
	section data {
		int x = 255
		int result = 0
		string fmt_str = " value is %b \n"
	}

	section code {
	start:
		and result, x, 0xFF
		print fmt_str, result
		xor result, x, 0xF
		print fmt_str, result
		or result, 0xFF
		print fmt_str, result
		not result
		print fmt_str, result
	stop:
		done
	}
}
